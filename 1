#include <iostream>
using namespace std;
int main()
{
    const int N = 10;
    int K, L, A[N];
    float s;
    cout << "Enter K, L: \n";
    cin >> K >> L;
    for (int i = 0; i < N; i++)
        A[i] = i;
    s = 0;
    for (int i = K; i <= L; i++) {
        s += A[i];
    }
    s = s / (L - K + 1);
    cout << s;
}
