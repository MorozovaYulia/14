#include <iostream>
using namespace std;
int main()
{
    const int N = 10;
    int A[N];
    int max = -2147483648;
    for (int i = 0; i < N; i++)
        cin >> A[i];
    for (int i = 1; i < N - 1; i++)
        if ((A[i] > A[i - 1]) && (A[i] > A[i + 1]))
            max = A[i];
    if (max ==  -2147483648)
        cout << "number does not exist";
    else
        cout << max;
}
