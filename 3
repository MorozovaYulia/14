#include <iostream>
using namespace std;  
int main()
{
    const int N = 10;
    int A[N], min, s;
    for (int i = 1; i <= N; i++)
        cin >> A[i];
    min = A[2];
    if (N % 2 == 0)
        s = N;
    else
        s = N - 1;
    for (int i = 0; i < s; i += 2)
        if (min > A[i])
            min = A[i];
    cout << min << " \n";
}
