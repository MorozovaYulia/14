#include <iostream>
using namespace std;
int main()
{
    const int N = 10;
    int A[N], a, b;
    for (int i = 0; i < N; i++)
        cin >> A[i];
    a = A[1] - A[0];
    b = 0;
    for (int i = 1; i < N; i++) {
        if (A[i] - A[i - 1] == A)
            b++;
    }
    if (b == N - 1)
        cout << a << " \n";
    else
        cout << 0 << " \n";
}
